{
  "name": "login_page",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://pdoubleu.github.io/login_page",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "blk-design-system-react": "^1.2.0",
    "bootstrap": "^4.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.12.2",
    "react-idle-timer": "^4.6.4",
    "react-scripts": "4.0.3",
    "reactstrap": "^8.9.0",
